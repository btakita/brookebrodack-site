    "cssnano": "^6.0.3",
    "ctx-core": "^6.1.2",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "elysia": "^0.8.16",
    "esmcss": "^0.1.8",
    "relementjs": "^0.70.1",
    "relysjs": "^1.11.2",
    "tailwindcss-noscript": "^2.0.1"
  },
  "devDependencies": {
    "@ctx-core/source-map": "^0.2.43",
    "@rebuildjs/tailwindcss": "^0.8.16",
    "bun-types": "latest",
    "esbuild-style-plugin": "^1.6.3",
    "picocolors": "^1.0.0",
