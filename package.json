{
  "name": "@btakita/brookebrodack-site",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build.ts",
    "clear": "rm -rf ./dist",
    "dev": "bun run dev:map",
    "dev:map": "./bin/dev.ts",
    "dev:raw": "bun index.ts",
    "db:generate": "drizzle-kit generate:sqlite",
