{
  "name": "@btakita/brookebrodack-site",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build.ts",
    "clear": "rm -rf ./dist",
    "dev": "bun run dev:sourcemap__resolve__bun",
    "dev:raw": "bun index.ts",
    "dev:sourcemap__resolve__bun": "bin/sourcemap__resolve__bun.ts -f index.ts",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "bun ./db/migrate.ts",
    "db:push": "drizzle-config push:sqlite",
    "start": "bin/sourcemap__resolve__bun.ts -f start.ts",
    "clean": "rm -rf ./node_modules/.vite"
  },
  "dependencies": {
    "@auth/core": "^0.27.0",
    "@btakita/domain--server--brookebrodack": "workspace:*",
    "@btakita/ui--browser--brookebrodack": "workspace:*",
    "@btakita/ui--server--brookebrodack": "workspace:*",
    "@rappstack/domain--server": "workspace:*",
    "@rappstack/domain--server--auth": "workspace:*",
    "cssnano": "^6.0.3",
    "ctx-core": "^6.4.2",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "elysia": "^0.8.17",
    "esmcss": "^0.1.9",
    "relementjs": "^0.73.2",
    "relysjs": "^1.14.1",
    "tailwindcss-noscript": "^2.0.1"
  },
  "devDependencies": {
    "@ctx-core/preprocess": "^0.1.2",
    "@ctx-core/source-map": "^0.2.49",
    "@rebuildjs/tailwindcss": "^0.8.40",
    "bun-types": "latest",
    "esbuild-style-plugin": "^1.6.3",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  }
}