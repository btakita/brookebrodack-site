---
import { brookers__page_c_ } from '@btakita/ui--server--brookebrodack'
import R from 'relementjs/astro'
import { server__ctx__new } from '../../lib/index.js'
import Layout from '../../layouts/Layout.astro'
let ctx = server__ctx__new(Astro)
---
<Layout>
	<R $={brookers__page_c_({ ctx })}/>
</Layout>

<script>
	import { browser__ctx__ensure } from '@btakita/domain--browser--brookebrodack'
	import { brookers__page__onbind } from '@btakita/ui--any--brookebrodack'
	import { brookers__page__onbind__init } from '@btakita/ui--browser--brookebrodack'
	import { dom__onbind__register } from '@ctx-core/dom'
	brookers__page__onbind__init()
	dom__onbind__register(browser__ctx__ensure(), brookers__page__onbind)
</script>
